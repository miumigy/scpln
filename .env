# Authentication (enable API key auth)
AUTH_MODE=apikey
API_KEY_HEADER=X-API-Key
API_KEY_VALUE=dev-secret

# Registry/DB
REGISTRY_BACKEND=db
SCPLN_DB=data/scpln.db
RUNS_DB_MAX_ROWS=1000

# Jobs
JOBS_ENABLED=1
JOBS_WORKERS=1
JOBS_BACKEND=memory

# Logging
SIM_LOG_LEVEL=INFO
#SIM_LOG_JSON=0

# Debug endpoints off by default
ENABLE_DEBUG_ENDPOINTS=0

